#map {
  width: 100%;
  height: 100vh;
}

.moveToLocation {
  width: 50px;
  height: 50px;
  background: url(http://oqlncjnqc.bkt.clouddn.com/location.png);
  background-position: 0 0;
  background-size: 100% 100%;
  margin: 0;
  border: 0;
  padding: 0;
  position: fixed;
  bottom: 15px;
  left: 10px;
}

.moveToLocation:after {
  content: none;
}

.map-button {
  width: 80px;
  height: 30px;
  line-height: 30px;
  background: #13a0a9;
  border: 0;
  padding: 0;
  border-radius: 15px;
  font-size: 12px;
  color: aliceblue;
}

.button1 {
  position: fixed;
  bottom: 95px;
  right: 10px;
}

.button2 {
  position: fixed;
  bottom: 55px;
  right: 10px;
}

.button3 {
  position: fixed;
  bottom: 15px;
  right: 10px;
}